 			"arc", "arz", "ar", "bcc", "bqi", "ckb", "dv", "fa", "glk", "ha", "he",
 			"khw", "ks", "mzn", "pnb", "ps", "sd", "ug", "ur", "yi"
 		];
 		return $.inArray(lang, rtl_langs) !== -1;
 	}
 	return {
 		initLanguages: initLanguages,
